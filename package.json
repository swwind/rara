{
  "private": true,
  "name": "rara",
  "scripts": {
    "build": "sass --no-source-map styles:app/styles && remix build",
    "dev": "concurrently -n \"REMIX,SASS,NODE\" \"remix watch\" \"sass --no-source-map --watch styles:app/styles\" \"yarn start:dev\"",
    "fmt": "prettier --write . && prisma format",
    "clean": "rm -rf public/build server/build .cache app/styles",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/index.js"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@remix-run/express": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "highlight.js": "^11.4.0",
    "katex": "^0.15.2",
    "markdown-it": "^12.3.2",
    "markdown-it-katex": "^2.0.3",
    "morgan": "^1.10.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "sanitize-html": "^2.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/markdown-it": "^12.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/sanitize-html": "^2.6.2",
    "concurrently": "^7.0.0",
    "esbuild-register": "^3.3.2",
    "prisma": "^3.9.2",
    "sass": "^1.49.7",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "katex": "0.15.2"
  },
  "engines": {
    "node": "=16"
  },
  "sideEffects": false
}
