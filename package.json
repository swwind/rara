{
  "private": true,
  "name": "rara",
  "scripts": {
    "build": "sass --no-source-map styles:app/styles && remix build",
    "dev": "concurrently -n \"REMIX,SASS,NODE\" \"remix watch\" \"sass --no-source-map --watch styles:app/styles\" \"yarn start:dev\"",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint --ext ts,tsx app server -f pretty --fix",
    "lint:check": "eslint --ext ts,tsx app server -f pretty",
    "clean": "rm -rf public/build server/build .cache app/styles",
    "postinstall": "patch-package && remix setup node && prisma generate",
    "start": "cross-env NODE_ENV=production node --require esbuild-register server/index.ts",
    "start:dev": "cross-env NODE_ENV=development node --require esbuild-register server/index.ts"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "gitHooks": {
    "pre-commit": "yarn && yarn fmt:check && yarn lint:check && yarn tsc"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@remix-run/express": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "katex": "^0.15.3",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.2",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-highlight": "^5.0.2",
    "rehype-katex": "^6.0.2",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-slug": "^5.0.1",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remix": "^1.3.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/md5": "^2.3.2",
    "@types/morgan": "^1.9.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.12.0",
    "eslint-formatter-pretty": "^4.1.0",
    "prettier-plugin-prisma": "^3.12.0",
    "prisma": "^3.12.0",
    "sass": "^1.49.7",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": "=16"
  },
  "sideEffects": false
}
